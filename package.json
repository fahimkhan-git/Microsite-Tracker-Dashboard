{
  "name": "microsite-tracker-dashboard",
  "version": "1.0.0",
  "description": "Real-time microsite tracking dashboard for Google Ads visitors and leads",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "client": "cd client && npm start",
    "client:build": "cd client && npm run build",
    "prisma:migrate": "npx prisma migrate dev",
    "prisma:generate": "npx prisma generate",
    "prisma:studio": "npx prisma studio",
    "prisma:deploy": "npx prisma migrate deploy",
    "setup": "npm install && cd client && npm install && cd .. && npx prisma generate",
    "build": "npm install && cd client && npm install && cd .. && npx prisma generate && npm run client:build"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "keywords": ["dashboard", "tracking", "google-ads", "gtm"],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "@prisma/client": "^5.7.1",
    "body-parser": "^1.20.2",
    "dotenv": "^16.3.1",
    "ws": "^8.14.2",
    "node-cron": "^3.0.3"
  },
  "devDependencies": {
    "nodemon": "^3.0.3",
    "prisma": "^5.7.1"
  }
}
